object HelloWorld {
	def main(args: Array[String]): Unit = {
	var ar = new Array[String](3)
	// var arr = Array("af","fd","afd" , "afsd")
	ar(0) = "abc"
	ar(1) = "gdf"
	ar(2) = "gtyr"
	
	var i =0
	for(i <- 0 to 2)
	println(ar(i))
	
	}
}



